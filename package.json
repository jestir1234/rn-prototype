{
  "name": "GoReadyMade",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "asyncstorage-mock": "^1.0.1",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "detox": "^7.3.5",
    "enzyme": "^3.3.0",
    "fetch-mock": "^6.3.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^22.4.3",
    "jest-expo": "26.0.0",
    "jest-fetch-mock": "^1.5.0",
    "mocha": "^5.1.1",
    "react-devtools": "^3.2.1",
    "react-native-scripts": "1.13.1",
    "react-test-renderer": "^16.3.2"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "react-devtools": "react-devtools",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-devtools-core|native-base-shoutem-theme|@shoutem/animation|@shoutem/ui|tcomb-form-native|react-redux|react-native-drawer)"
    ]
  },
  "dependencies": {
    "cross-fetch": "^2.1.0",
    "expect": "^22.4.3",
    "expo": "^26.0.0",
    "mock-async-storage": "^2.0.2",
    "native-base": "^2.4.1",
    "npm": "^5.8.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-native": "0.54.0",
    "react-redux": "5.0.7",
    "react-native-calendars": "^1.18.2",
    "react-native-default-preference": "^1.3.1",
    "react-navigation": "^1.5.11",
    "recyclerlistview": "^1.3.2",
    "redux-thunk": "^2.2.0",
    "redux-mock-store": "^1.5.1"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/GoReadyMeal.app",
        "build": "xcodebuild -project ios/GoReadyMeal.xcodeproj -scheme GoReadyMeal -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.attached",
        "name": "N5X"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
        "type": "android.emulator",
        "name": "N5X"
      }
    }
  }
}
